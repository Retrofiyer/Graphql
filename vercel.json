{
    "version": 2,
    "builds": [
        {
          "src":"./index.js",
          "use":"@vercel/node"
        },
        {
          "src":"./index.html",
          "use":"@vercel/static"
        }
    ],
    "routes": [
        {
            "src":"/graphql",
            "dest":"index.js"
        },
        {
            "src":"/graphql(.*)",
            "dest":"index.js"
        },
        {
            "src":"/(.*)",
            "dest":"/index.html"
        }
    ]
}